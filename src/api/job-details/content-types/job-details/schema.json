{
  "kind": "collectionType",
  "collectionName": "jobs_details",
  "info": {
    "singularName": "job-details",
    "pluralName": "jobs-details",
    "displayName": "Job Details"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "content": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "standard"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "label": {
      "type": "string"
    },
    "job": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::job.job",
      "mappedBy": "job_detail"
    }
  }
}
